{
    parser: 'babel-eslint',
    extends: 'airbnb',
    plugins: [
        'react'
    ],
    rules: {
        'linebreak-style': 0,
        'no-tabs': 0,
        'indent': 0,
        'react/jsx-indent': ['error', 4],
        'react/jsx-filename-extension': 0,
        'react/prop-types': 0
    },
    globals: {
        'window': true,
        '__DEV__': true
    }
}
